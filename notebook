            "source": [
                "Complex Query\n",
                "\n",
                "  \n",
                "\n",
                "\\- Grouping and statistical Aggregation (group -\\> select -\\> order)\n",
                "\n",
                "\\- Nested Query (intermediate table return value)"
            ],
            "metadata": {
                "language": "sql",
                "azdata_cell_guid": "6009d40a-171b-4ad4-bf0a-92f0507380be"
            },
            "attachments": {}
        },
        {
            "cell_type": "code",
            "source": [
                "SELECT BranchNo, COUNT(StaffID) as StaffCount, SUM(Salary) as SumOfSalary FROM Staff \n",
                "WHERE \n",
                "\n",
                "\n",
                "Salary > (SELECT AVG(Salary) FROM STAFF) \n",
                "AND BranchNo IN (SELECT BranchNo FROM Branch WHERE Address LIKE '%Rose%')\n",
                "GROUP BY BranchNo HAVING BranchNo > 1 and SUM(Salary) > 400 ORDER BY BranchNo DESC;"
            ],
            "metadata": {
                "azdata_cell_guid": "56f3b266-842c-4f39-8d0c-2ff80744e048",
